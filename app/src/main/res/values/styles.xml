<resources>


    <!--app start theme that has not title bar and state bar-->
    <style name="Theme.AppStart" parent="android:Theme.Black.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <!--<item name="android:elevation">5dp</item>-->
    </style>

    <style name="custom_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_checkbox</item>
    </style>

    <style name="vote_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/radio_btn_selector</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

</resources>
